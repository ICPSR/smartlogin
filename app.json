{
  "expo": {
    "name": "ICPSR Smart Login",
    "description": "An easy way to login to ICPSR's website.",
    "slug": "icpsr-smart-login",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#005050"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.icpsr.smartlogin",
      "supportsTablet": true
    },
    "android": {
      "package": "com.icpsr.smartlogin"
    }
  }
}
